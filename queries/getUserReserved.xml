<query id="getUserReserved" action="select">
	<tables>
		<table name="notifymessage_reservation" />
	</tables>
	<columns>
		<column name="*" />
	</columns>
	<conditions>
		<condition operation="equal" column="cellphone" var="cellphone" notnull="notnull"  />
		<condition operation="equal" column="reservdate" var="reservdate" pipe="and" />
	</conditions>
</query>

